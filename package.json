{
  "name": "ahmad1284.github.io",
  "version": "0.0.1",
  "description": "Ahmad's personal website",
  "scripts": {
    "watch:sass": "sass --watch ./src/_sass:src/_css",
    "build:sass": "sass ./src/_sass:src/_css",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:eleventy": "npx @11ty/eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy --serve",
    "debughard": "DEBUG=Dev:Eleventy* npx @11ty/eleventy --serve"
  },
  "author": "ahmad1284",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0"
  }
}
